<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="CFapp" table="app_table">
        <meta attribute="class-description">
            This class contains CFapp details.
        </meta>
        <id name="id" type="int" column="app_id">
            <generator class="native"/>
        </id>
        <property name="key" column="app_key" type="string"/>
        <property name="value" column="app_val" type="string"/>
    </class>
</hibernate-mapping>
